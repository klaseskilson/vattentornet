<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>email</th>
      <th>admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.admin %></td>
        <td><%= link_to 'Redigera', edit_user_path(user) %></td>
        <td><%= link_to 'Ta bort', user_path(user), data: { confirm: "Ã„r du sÃ¤ker?" }, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

